{
  "name": "starikovm_portfolio_website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "publish": "vue-cli-service build && cd ../starikovm.github.io/ && rm -rf * && cp -R ../starikovm_portfolio_website/dist/* ./ && git add . && git commit -m 'publish' && git push",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "pts": "^0.5.0",
    "register-service-worker": "^1.0.0",
    "vue": "^2.5.17",
    "vue-meta": "^2.4.0",
    "vue-router": "^3.0.1",
    "vue-scrollto": "^2.20.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0",
    "@vue/cli-plugin-eslint": "^3.0.0",
    "@vue/cli-plugin-pwa": "^3.0.0",
    "@vue/cli-service": "^3.0.0",
    "@vue/eslint-config-prettier": "^3.0.0",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
